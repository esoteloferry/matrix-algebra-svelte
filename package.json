{
	"name": "matrix-algebra-svelte",
	"version": "0.0.13",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test-scripts": "jest",
		"prepublish": "npm run package",
		"publish": "npm publish ./package",
		"prerelease": "npm run format && npm run test-scripts",
		"release": "standard-version",
		"postrelease": "git push --follow-tags origin main",
		"release-package": "npm run release && npm run publish"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/jest": "^27.4.0",
		"autoprefixer": "^10.4.0",
		"jest": "^27.4.5",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"standard-version": "^9.3.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.4.12",
		"tailwindcss": "^3.0.7",
		"ts-jest": "^27.1.2",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/esoteloferry/matrix-algebra-svelte.git"
	},
	"homepage": "https://github.com/esoteloferry/matrix-algebra-svelte.git",
	"license": "MIT"
}
